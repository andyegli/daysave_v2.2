{
  "name": "daysave-app",
  "version": "1.0.0",
  "description": "DaySave.app - Save and organize your daily discoveries",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "debug": "node --inspect=0.0.0.0:9229 src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js views/**/*.ejs public/**/*.{js,css}",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:dev": "cross-env NODE_ENV=development sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:dev": "cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "setup:dev": "npm run migrate:dev && npm run seed:dev",
    "build": "npm run lint && npm run test",
    "deploy:gcp": "gcloud app deploy",
    "generate:docs": "jsdoc src/ -r -d docs/jsdoc",
    "plantuml": "plantuml docs/diagrams/src/*.puml -o ../out/",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && npm run migrate:dev && npm run seed:dev"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "ejs": "^3.1.9",
    "cookie-parser": "^1.4.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-facebook": "^3.0.0",
    "passport-microsoft": "^1.0.0",
    "passport-oauth2": "^1.7.0",
    "@simplewebauthn/server": "^8.3.6",
    "@simplewebauthn/browser": "^8.3.5",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "otplib": "^12.0.1",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "redis": "^4.6.10",
    "connect-redis": "^7.1.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "stripe": "^14.9.0",
    "paypal-rest-sdk": "^1.8.1",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "sharp": "^0.33.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vision": "^4.0.2",
    "@google-cloud/translate": "^8.0.2",
    "@google-cloud/language": "^6.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto-js": "^4.2.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "express-unless": "^2.1.3",
    "geoip-lite": "^1.4.8",
    "useragent": "^2.3.0",
    "device-detector-js": "^3.0.3",
    "express-mongo-sanitize": "^2.2.0",
    "xss": "^1.0.14",
    "hpp": "^0.2.3",
    "connect-flash": "^0.1.1",
    "express-handlebars": "^7.1.2",
    "i18n": "^0.15.1",
    "validator": "^13.11.0",
    "sanitize-html": "^2.11.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "node-schedule": "^2.1.1",
    "express-slow-down": "^2.0.1",
    "express-brute": "^1.0.1",
    "fingerprintjs2": "^2.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@playwright/test": "^1.40.1",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.1",
    "jsdoc": "^4.0.2",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyegli/daysave.app.git"
  },
  "keywords": [
